double readtemp() {
  double Vout, Rt = 0;
  double T, Tc, Tf = 0;

  double adc = 0;
  adc = analogRead(ThermistorPin);

  //double adc1 = 0;
  //adc1 = analogRead(34);

  adc = ADC_LUT[(int)adc];


  Vout = adc * Vs / adcMax;
  Rt = (R1 * Vout) / (Vs - Vout);

  T = 1 / (1 / To + log(Rt / Ro) / Beta);  // Temperature in Kelvin
  Tc = T - 273.15;                         // Celsius
  Tf = Tc * 9 / 5 + 32;                    // Fahrenheit
  //if (Tc > 0) Serial.println(Tc);
  if(isnan(Tc))
  {
    Tc = lastRead;
  }
  else
  {
	lastRead = Tc;
  }
  return Tc+deltaTemperature;
}

static float lastExtReadTemp = 15;
bool FirstReadExtTemp = true;
double filteredTemp = 0;
double alpha = 0.1; // Regola la stabilità
double readtempEXT() {
  double Vout, Rt = 0;
  double T, Tc, Tf = 0;

  double adc = 0;
  adc = analogRead(ThermistorPinEXT);

  //double adc1 = 0;
  //adc1 = analogRead(34);

  adc = ADC_LUT[(int)adc];


  Vout = adc * Vs / adcMax;
  Rt = (R1 * Vout) / (Vs - Vout);

  T = 1 / (1 / To + log(Rt / Ro) / Beta);  // Temperature in Kelvin
  Tc = T - 273.15;                         // Celsius
  Tf = Tc * 9 / 5 + 32;                    // Fahrenheit
  //if (Tc > 0) Serial.println(Tc);
  
  if(isnan(Tc))
  {
    Tc = lastExtReadTemp;
  }
  else
  {
	  lastExtReadTemp = Tc;
  }

  if(FirstReadExtTemp)
  {
    FirstReadExtTemp = false;
    filteredTemp = Tc;
  }
  else
  {
    filteredTemp = alpha * Tc + (1 - alpha) * filteredTemp;  // Filtro
  }

  return filteredTemp;
}